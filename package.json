{
    "name": "@etsoo/appscript",
    "version": "1.0.64",
    "description": "Applications shared TypeScript framework",
    "main": "lib/index.js",
    "types": "lib/index.d.ts",
    "scripts": {
        "build": "tsc",
        "format": "prettier --write src/**/*.ts",
        "lint": "eslint --ext .ts src/",
        "test": "jest",
        "test:debug": "node --inspect-brk ./node_modules/jest/bin/jest.js --runInBand"
    },
    "jest": {
        "automock": false,
        "testMatch": [
            "<rootDir>/__tests__/**/*.ts"
        ],
        "testEnvironment": "jsdom",
        "transform": {
            ".+\\.jsx?$": "babel-jest",
            ".+\\.tsx?$": "ts-jest"
        },
        "transformIgnorePatterns": [
            "/node_modules/(?!@etsoo/).+\\.js$"
        ]
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/ETSOO/NotificationBase.git"
    },
    "keywords": [
        "notification",
        "TypeScript"
    ],
    "author": "Garry Xiao",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/ETSOO/NotificationBase/issues"
    },
    "homepage": "https://github.com/ETSOO/NotificationBase#readme",
    "dependencies": {
        "@etsoo/notificationbase": "^1.0.68",
        "@etsoo/restclient": "^1.0.39",
        "@etsoo/shared": "^1.0.26",
        "typescript": "^4.3.2"
    },
    "devDependencies": {
        "@babel/cli": "^7.14.5",
        "@babel/core": "^7.14.5",
        "@babel/plugin-transform-runtime": "^7.14.5",
        "@babel/preset-env": "^7.14.5",
        "@babel/runtime-corejs3": "^7.14.5",
        "@types/jest": "^26.0.23",
        "@typescript-eslint/eslint-plugin": "^4.26.1",
        "@typescript-eslint/parser": "^4.26.1",
        "eslint": "^7.28.0",
        "eslint-config-airbnb-base": "^14.2.1",
        "eslint-plugin-import": "^2.23.4",
        "jest": "^27.0.4",
        "ts-jest": "^27.0.3"
    }
}
